{
    "name": "sectik",
    "displayName": "Sectik",
    "description": "My VS Code extension",
    "version": "0.0.1",
    "engines": {
      "vscode": "^1.60.0"
    },
    "categories": [
      "Other"
    ],
    "repository": {
      "type": "git",
      "url": ""
    },
    "activationEvents": [
      "onWebviewView:sectik-sidebar"
    ],
    "main": "./dist/extension.js",
    "contributes": {
      "viewsContainers": {
        "activitybar": [
          {
            "id": "sectik-sidebar-view",
            "title": "Sectik",
            "icon": "media/icon.svg"
          }
        ]
      },
      "views": {
        "sectik-sidebar-view": [
          {
            "type": "webview",
            "id": "sectik-sidebar",
            "name": "Sectik",
            "icon": "media/icon.svg",
            "contextualTitle": "Sectik"
          }
        ]
      },
      "commands": [
        {
          "command": "sectik.sayhello",
          "category": "Sectik",
          "title": "Say hello"
        }
      ]
    },
    "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && rollup -c",
    "clean": "rimraf out dist",
  "build": "npm run clean && npm run compile",
    "watch": "rollup -c -w",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
    "dependencies": {
      "tiktoken": "^1.0.15",
      "vscode": "*"
    },
    "devDependencies": {
      "@rollup/plugin-commonjs": "^20.0.0",
      "@rollup/plugin-node-resolve": "^13.0.4",
      "@rollup/plugin-typescript": "^8.2.5",
      "@tsconfig/svelte": "^2.0.1",
      "@types/glob": "^7.1.3",
      "@types/mocha": "^8.2.2",
      "@types/node": "14.x",
      "@types/polka": "^0.5.3",
      "@types/vscode": "^1.91.0",
      "@typescript-eslint/eslint-plugin": "^4.26.0",
      "@typescript-eslint/parser": "^4.26.0",
      "concurrently": "^6.2.1",
      "eslint": "^7.27.0",
      "glob": "^7.1.7",
      "mocha": "^8.4.0",
      "rollup": "^2.56.3",
      "rollup-plugin-css-only": "^4.5.2",
      "rollup-plugin-svelte": "^7.2.2",
      "rollup-plugin-terser": "^7.0.2",
      "svelte": "^3.59.2",
      "svelte-check": "^2.2.6",
      "svelte-preprocess": "^4.10.7",
      "ts-loader": "^9.2.2",
      "typescript": "^4.9.5",
      "vscode-test": "^1.5.2",
      "webpack": "^5.38.1",
      "webpack-cli": "^4.7.0"
    },
    "homepage": "https://github.com/sfabio01/vscode-extension-template/blob/main/README.md"
  }
  